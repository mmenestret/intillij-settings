          <ShortCut>'Ctrl+Maj+F10'</ShortCut>
          <Description>Run 'RoutesSpec.authentic...'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'ScalaTests in 'scala''">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
