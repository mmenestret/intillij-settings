<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="File Structure">
        <Item>
          <IdeaActionID>FileStructurePopup</IdeaActionID>
          <ShortCut>'Ctrl+F12'</ShortCut>
          <Description>File Structure</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Recompile 'App.scala'">
        <Item>
          <IdeaActionID>Compile</IdeaActionID>
          <ShortCut>'Ctrl+Maj+F9'</ShortCut>
          <Description>Recompile 'App.scala'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'App'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'Ctrl+Maj+F10'</ShortCut>
          <Description>Run 'App'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'ScalaTests in 'scala''">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Maj+F10'</ShortCut>
          <Description>Run 'ScalaTests in 'scala''</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Diagram...">
        <Item>
          <IdeaActionID>ShowUmlDiagram</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Maj+U'</ShortCut>
          <Description>Show Diagram...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="GoTo">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B'</ShortCut>
          <Description>GoTo</Description>
          <Count>7</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>