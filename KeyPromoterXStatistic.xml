<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="File Structure">
        <Item>
          <IdeaActionID>FileStructurePopup</IdeaActionID>
          <ShortCut>'Ctrl+F12'</ShortCut>
          <Description>File Structure</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
